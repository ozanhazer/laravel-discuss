{
    "name": "alfatron/laravel-discuss",
    "description": "Discussions for Laravel",
    "keywords": [
        "alfatron",
        "discussions",
        "laravel",
        "forum"
    ],
    "homepage": "https://github.com/alfatron/laravel-discuss",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ozan Hazer",
            "email": "ozanhazer@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Alfatron\\Discuss\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alfatron\\Discuss\\Tests\\": "tests"
        },
        "classmap": [
            "tests/models/User.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html storage/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alfatron\\Discuss\\DiscussServiceProvider"
            ],
            "aliases": {
                "Discuss": "Alfatron\\Discuss\\DiscussFacade"
            }
        }
    }
}
